{
  "name": "panlatent/site-cli",
  "description": "Help you manage Nginx local development configuration",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "panlatent",
      "email": "panlatent@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5",
    "panlatent/boost": "~0.2",
    "symfony/console": "~3.0",
    "symfony/finder": "~3.0",
    "symfony/filesystem": "~3.0",
    "symfony/yaml": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
